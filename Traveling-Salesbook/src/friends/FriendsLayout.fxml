          <TableColumn prefWidth="106.0" text="First Name" />
          <TableColumn prefWidth="119.0" text="Last Name" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="240.0" layoutY="491.0" mnemonicParsing="false" onMouseClicked="#addFriend" text="ADD">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="304.0" layoutY="491.0" mnemonicParsing="false" onMouseClicked="#removeFriend" text="Remove">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="393.0" layoutY="491.0" mnemonicParsing="false" onMouseClicked="#showFriendPosts" text="Show Post">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
